{{#each this}}
<li>
  <div class="film-card">
  <img class="film-picture" src="{{poster_path}}" alt="film card" />
</div>
<div class="film-info">
  <h2 class="film-name">{{original_title}}</h2>
  <p class="film-genre">{{genre_ids}}</p>
  <p class="film-year">{{release_date}}</p>
  <span class="vote">{{vote_average}}</span>
</div>
</li>
{{/each}}